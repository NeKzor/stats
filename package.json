{
    "name": "stats",
    "version": "1.0.0",
    "author": "NeKz",
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build -m Update",
        "start": "react-scripts start",
        "build": "react-scripts build && cp build/index.html build/404.html",
        "test": "react-scripts test --env=jsdom",
        "clone:api": "git clone --single-branch --branch api https://github.com/NeKzBot/iverb-stats api",
        "clone:build": "git clone --single-branch --branch gh-pages https://github.com/NeKzor/stats build",
        "dev": "hs ./api --cors",
        "prettier": "prettier --config ./prettier.config.js --write './src/**/*.js' './scripts/**/*.js'",
        "audit": "npm audit --production"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "apexcharts": "^3.26.2",
        "chart-race-react": "^1.0.2",
        "d3-scale": "^3.2.3",
        "gh-pages": "^2.2.0",
        "moment": "^2.29.1",
        "react": "^17.0.2",
        "react-apexcharts": "^1.3.7",
        "react-dom": "^17.0.2",
        "react-json-view": "^1.21.3",
        "react-moment": "^0.9.7",
        "react-router": "^3.2.6",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "react-scripts": "^3.4.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
