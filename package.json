        "apexcharts": "^3.26.2",
        "chart-race-react": "^1.0.2",
        "d3-scale": "^3.2.3",
        "gh-pages": "^2.2.0",
        "moment": "^2.29.4",
        "react": "^17.0.2",
        "react-apexcharts": "^1.3.7",
        "react-dom": "^17.0.2",
        "react-json-view": "^1.21.3",
